{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug: cmd/envmux",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "hideSystemGoroutines": true,
      "showGlobalVariables": true,
      "showLog": true,
      "showRegisters": false,
      "preLaunchTask": "generate: parser",
      "program": "${workspaceFolder}/cmd/envmux",
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "buildFlags": [
        "-tags=pprof",
        "-gcflags=-N -l"
      ],
      "env": {},
      "args": [],
    },
  ]
}
