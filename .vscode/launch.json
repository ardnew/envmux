{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug: cmd/envmux",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "envFile": "${workspaceFolder}/.vscode/launch.env",
      "hideSystemGoroutines": true,
      "showGlobalVariables": true,
      "showLog": true,
      "showRegisters": false,
      "preLaunchTask": "generate: parser",
      "program": "${workspaceFolder}/cmd/envmux",
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "buildFlags": [
        "-tags=${env:buildFlagsTags}",
        "-gcflags=${env:buildFlagsGCFlags}"
      ],
      "env": {},
      "args": [],
    },
    {
      "name": "debug: spec/parse",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "envFile": "${workspaceFolder}/.vscode/launch.env",
      "hideSystemGoroutines": true,
      "showGlobalVariables": true,
      "showLog": true,
      "showRegisters": false,
      "preLaunchTask": "generate: parser",
      "program": "${workspaceFolder}/spec/parse",
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "buildFlags": [
        "-tags=${env:buildFlagsTags}",
        "-gcflags=${env:buildFlagsGCFlags}"
      ],
      "env": {},
      "args": [
        "-test.v",
        "-test.run=^TestParser$"
      ],
    },
    {
      "name": "test: cmd/envmux",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "envFile": "${workspaceFolder}/.vscode/launch.env",
      "hideSystemGoroutines": true,
      "showGlobalVariables": true,
      "showLog": true,
      "showRegisters": false,
      "preLaunchTask": "generate: parser",
      "program": "${workspaceFolder}/cmd/envmux",
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "buildFlags": [
        "-tags=${env:buildFlagsTags}",
        "-gcflags=${env:buildFlagsGCFlags}"
      ],
      "env": {},
      "args": [
        "-test.v",
        "./...",
      ],
    },
    {
      "name": "debug: cmd/envmux (simple)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "envFile": "${workspaceFolder}/.vscode/launch.env",
      "hideSystemGoroutines": true,
      "showGlobalVariables": true,
      "showLog": true,
      "showRegisters": false,
      "preLaunchTask": "generate: parser",
      "program": "${workspaceFolder}/cmd/envmux",
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "buildFlags": [
        "-tags=${env:buildFlagsTags}",
        "-gcflags=${env:buildFlagsGCFlags}"
      ],
      "env": {},
      "args": [
        "-p",
        "trace=.",
        "-i",
        "-j",
        "1",
        "-v",
        "-u",
        "-d",
        "default<foo(\"8.2\")>{x=1}",
        "-d",
        "foo{y=_+\"1\"}",
      ],
    },
    {
      "name": "debug: cmd/envmux (composite)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "envFile": "${workspaceFolder}/.vscode/launch.env",
      "hideSystemGoroutines": true,
      "showGlobalVariables": true,
      "showLog": true,
      "showRegisters": false,
      "preLaunchTask": "generate: parser",
      "program": "${workspaceFolder}/cmd/envmux",
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "buildFlags": [
        "-tags=${env:buildFlagsTags}",
        "-gcflags=${env:buildFlagsGCFlags}"
      ],
      "env": {},
      "args": [
        "-j",
        "1",
        "-i",
        "-u",
        "-d",
        "=default{HOME=cwd()}m<default>(\"/usr/local\",\"/usr\"){HOME=mung.prefixif(HOME,file.isDir,_)}",
        "m",
      ],
    },
  ]
}