  "configurations": [
    {
      "name": "debug: cmd/envmux",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "preLaunchTask": "generate: lexer",
      "program": "./cmd/envmux",
      "buildFlags": [
        "-tags=pprof",
        "-gcflags=-N -l" // -m for escape analysis
      ],
      "env": {},
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
    },
    {
      "name": "test: cmd/envmux",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "preLaunchTask": "generate: lexer",
      "program": "./cmd/envmux",
      "env": {},
      "args": [
        "-test.v",
        "-test.run=TestMain",
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
    },
    {
      "name": "debug: cmd/envmux (simple)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "preLaunchTask": "generate: lexer",
      "program": "./cmd/envmux",
      "buildFlags": [
        "-tags=pprof",
        "-gcflags=-N -l" // -m for escape analysis
      ],
      "env": {},
      "args": [
        "-p",
        "trace=.",
        "-i",
        "-j",
        "1",
        "-v",
        "-u",
        "-s",
        "=default<foo(\"xyz\"), bar>(){} foo(123) bar(\"abc\"){x=1;}",
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
    },
    {
      "name": "debug: cmd/envmux (composite)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "preLaunchTask": "generate: lexer",
      "program": "./cmd/envmux",
      "buildFlags": [
        "-tags=pprof",
        "-gcflags=-N -l" // -m for escape analysis
      ],
      "env": {},
      "args": [
        "-j",
        "1",
        "-i",
        "-u",
        "-s",
        "=default{HOME=cwd()}m<default>(\"/usr/local\",\"/usr\"){HOME=mung.prefixif(HOME,file.isDir,_)}",
        "m",
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
    }
  ]
}