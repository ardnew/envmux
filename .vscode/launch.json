  "configurations": [
    {
      "name": "debug: cmd/envmux",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "./cmd/envmux",
      "buildFlags": [
        "-tags=pprof",
        "-gcflags=github.com/ardnew/envmux/...=-N -l" // -m for escape analysis
      ],
      "env": {},
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
    },
    {
      "name": "test: cmd/envmux",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "./cmd/envmux",
      "env": {},
      "args": [
        "-test.v",
        "-test.run=TestMain",
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
    },
    {
      "name": "debug: cmd/envmux (simple)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "./cmd/envmux",
      "buildFlags": [
        "-tags=pprof",
        "-gcflags=github.com/ardnew/envmux/...=-N -l" // -m for escape analysis
      ],
      "env": {},
      "args": [
        "-p",
        "trace=.",
        "-j",
        "1",
        "-s",
        "=d{x=1}",
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
    },
    {
      "name": "debug: cmd/envmux (composite)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "./cmd/envmux",
      "buildFlags": [
        "-tags=pprof",
        "-gcflags=github.com/ardnew/envmux/...=-N -l" // -m for escape analysis
      ],
      "env": {},
      "args": [
        "-j",
        "1",
        "-i",
        "-s",
        "=default {\n  foo = 1+2;\n}\n; foo<default> {\n  bar = 3+4;\n}\n",
        "foo"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
    }
  ]
}