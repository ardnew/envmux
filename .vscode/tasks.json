{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "generate: parser",
      "type": "shell",
      "command": "go",
      "args": [
        "generate",
        "./spec/parse/..."
      ],
      "options": {
        "cwd": "${workspaceFolder}",
        "env": {},
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "never",
        "revealProblems": "onProblem",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": false,
        "close": true,
      },
      "isBackground": false,
    }
  ]
}